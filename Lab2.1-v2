#include <stdlib.h>
#include <stdio.h>
#include <math.h>
int main()
{
    double x, y;

    printf("Enter x:");
    scanf("%lf", &x);
    if( x>= 0 && x < 5)
    y = (x-1)/(x+1);
    else if ( x >= 5 && x < 25 )
    y = 5*x + 10;
    else if ( x >= 100 && x < 125 )
    y = (x/sqrt(x+pow(x,2)))-4*pow(x,3);
    else
    y = (x/10) + 4;
    system("cls");
    printf("x = %lf", x);
    printf("\ny = %lf", y);
    scanf("%d",y);
    return 0;

}
